<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="pop_bottom_style">
        <item name="android:windowEnterAnimation">@anim/set_pop_anim</item>
        <item name="android:windowExitAnimation">@anim/set_pop_anim_finish</item>
    </style>
    <style name="feedbackPopAnimation">
        <item name="android:windowEnterAnimation">@anim/set_pop_anim_enter</item>
        <item name="android:windowExitAnimation">@anim/set_pop_anim_enter_exit</item>
    </style>
</resources>
